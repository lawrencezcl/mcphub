{
  "name": "mcphub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate:pg",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit push:pg",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:seed": "dotenv -e .env.local -- tsx src/db/seed.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "crawl:add-sources": "dotenv -e .env.local -- tsx scripts/add-sources.ts",
    "crawl:local": "dotenv -e .env.local -- tsx scripts/crawl-local.ts",
    "crawl:safe": "dotenv -e .env.local -- tsx scripts/crawl-with-delay.ts",
    "crawl:npm": "dotenv -e .env.local -- tsx scripts/crawl-npm-only.ts",
    "llm:process": "dotenv -e .env.local -- tsx scripts/process-llm-queue.ts",
    "llm:test": "dotenv -e .env.local -- tsx scripts/test-llm-call.ts",
    "ingests:auto-approve": "dotenv -e .env.local -- tsx scripts/auto-approve-ingests.ts",
    "db:check": "dotenv -e .env.local -- tsx scripts/check-db-data.ts",
    "ingests:create": "dotenv -e .env.local -- tsx scripts/create-ingests.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "lucide-react": "^0.544.0",
    "nanoid": "^5.1.6",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "tailwind-merge": "^3.3.1",
    "use-debounce": "^10.0.6",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.19",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
